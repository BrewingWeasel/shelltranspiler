pub fun cursor_up(i: int) {
  @print("\033[{}A", i)
}

pub fun cursor_down(i: int) {
  @print("\033[{}B", i)
}

pub fun cursor_left(i: int) {
  @print("\033[{}D", i)
}

pub fun cursor_right(i: int) {
  @print("\033[{}C", i)
}

pub fun cursor_to_coords(x: int, y: int) {
  @print("\033[{};{}H", x, y)
}
