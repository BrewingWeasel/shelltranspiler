values = echo("find values")

fun print_three_times(value, other) {
  local local_value = "yes"
  global_value = "hi"
  echo(value)
  echo(value)
  echo(value)
}

echo(global_value); # this compiles
# But this would not
# echo(local_value)

if false() {
  echo("it works!")
} else if true() {
  echo("yes!")
}

echo("hi again"); echo("hi again again");

other_stuff = "lol"
echo(values, other_stuff)
print_three_times("lol")
