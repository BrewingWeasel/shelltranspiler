fun say_x_n_times(x: string, n: int) -> string {
  echo(n)
  echo(x)
  return x
}

value: int = 23;
str_value: string = "yes";

new_value: string = say_x_n_times(str_value, value);
echo(new_value)



say_x_n_times(say_x_n_times("yes!", 23), 2)

if (true() || false()) && (true() || false()) {
  echo("that is very true")
}

if find(".rs") && true() {
  files=fd(".rs") |> sed("s#src/#start/#")
  if files == "start/main.rs" {
    echo(files) |> lolcat()
  } else {
    echo("nope")
  }
}
